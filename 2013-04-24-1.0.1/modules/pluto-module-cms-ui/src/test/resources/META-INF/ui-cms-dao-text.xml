<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/aop 
http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
">

	<bean id="directoryDao"
		class="com.clican.pluto.cms.dao.hibernate.DirectoryDaoHibernateImpl"
		autowire="byName" />

	<bean id="dataModelDao"
		class="com.clican.pluto.cms.dao.hibernate.DataModelDaoHibernateImpl"
		autowire="byName" />
		
	<bean id="templateDao"
		class="com.clican.pluto.cms.dao.hibernate.TemplateDaoHibernateImpl"
		autowire="byName" />
		
	<bean id="commonDao"
		class="com.clican.pluto.cms.dao.hibernate.CommonDaoHibernateImpl"
		autowire="byName" />
		
	<bean id="siteDao"
		class="com.clican.pluto.cms.dao.hibernate.SiteDaoHibernateImpl"
		autowire="byName" />

</beans>
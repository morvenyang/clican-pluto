<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/profile_select_row_relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:duplicateParentState="true"
    android:minHeight="50dp"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/profile_select_row_iconImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="false"
        android:layout_centerVertical="true"
        android:layout_marginLeft="12dp"
        android:src="@drawable/ic_xysq" />

    <ImageView
        android:id="@+id/profile_select_row_arrowImageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="false"
        android:layout_centerHorizontal="false"
        android:layout_centerVertical="true"
        android:layout_marginRight="12dp"
        android:src="@drawable/go2_icon" />

    <TextView
        android:id="@+id/profile_select_row_titleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="false"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        android:layout_toLeftOf="@+id/profile_select_row_arrowImageView"
        android:layout_toRightOf="@+id/profile_select_row_iconImageView"
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/labelTextColor"
        android:textSize="15sp" />

</RelativeLayout>

		$("#create").submit( function () {
		var title = "《疑犯追踪 第2季》";
		var vid = $("#vid").val(),
			content = title+$("#content").val(),
			loading = $("#loading", window.parent.document);
			loading.show();
		$.get("/m/create.json",{ vid: vid, content: content }, function(data){
			loading.hide();
			$("#content").val('');
			apilist.comment('','1371137',1,1);
		});
		return false;
	});
});

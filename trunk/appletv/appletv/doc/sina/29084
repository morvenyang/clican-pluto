$(document).ready(function () {
	$(".pagebar").delegate("a", "click.onPageturn", function(e) {
		var page = $(this).attr("title");
		if (!page) {
			page = 1;
		}
		apilist.comment('','1371137',commentType,page);
	});
	
	//lazy load
	$("img.lazy").lazyload({
		effect : 'fadeIn'

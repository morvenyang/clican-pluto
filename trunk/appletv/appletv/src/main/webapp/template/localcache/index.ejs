<?xml version="1.0" encoding="UTF-8"?>
<atv>
<head>
<script src="<%=serverurl%>/javascript/load.js"/>
</head>
<body>
		<listScrollerSplit id="bbbb">
			<header>
				<simpleHeader horizontalAlignment="left">
					<title><![CDATA[离线缓存]]></title>
					<image src=""></image>
				</simpleHeader>
			</header>
			<menu>
				<sections>
					<menuSection>
						<items>
							<%for(var i=0;i<items.length;i++){%>
									<%if(items[i].url!=null&&items[i].url!=''){%>
									<oneLineMenuItem id="albumItem_<%=i%>" onSelect="localcacheClient.play('<%=items[i].url%>');">
										<label><![CDATA[<%=items[i].displayName%>]]></label>
									</oneLineMenuItem>
									<%}else{%>
									<oneLineMenuItem id="albumItem_<%=i%>" onSelect="appletv.showDialog('无法播放','缓存文件还未下载完成');">
										<label><![CDATA[<%=items[i].displayName%>]]></label>
									</oneLineMenuItem>
									<%}%>
							<%}%>
						</items>
					</menuSection>
				</sections>
			</menu>
		</listScrollerSplit>
	</body>
</atv>